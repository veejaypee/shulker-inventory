{
  "schemaVersion": 1,
  "id": "shulker-inventory",
  "version": "${version}",
  "name": "Shulker Inventory Bank",
  "description": "A mod for a centralized shulker inventory storage",
  "authors": [
    "Lobster"
  ],
  "contact": {
    "homepage": "https://github.com/veejaypee/shulker-inventory/issues",
    "sources": "https://github.com/veejaypee/shulker-inventory"
  },
  "license": "CC0-1.0",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "value": "com.lobster.shulker.manager.ShulkerInventory",
        "adapter": "kotlin"
      }
    ],
    "client": [
      {
        "value": "com.lobster.shulker.manager.ShulkerInventoryClient",
        "adapter": "kotlin"
      }
    ],
    "fabric-datagen": [
      {
        "value": "com.lobster.shulker.manager.ShulkerInventoryDataGenerator",
        "adapter": "kotlin"
      }
    ]
  },
  "mixins": [
    "shulker-inventory.mixins.json",
    {
      "config": "shulker-inventory.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=0.17.3",
    "minecraft": "~1.21.10",
    "java": ">=21",
    "fabric-api": "*",
    "fabric-language-kotlin": "*"
  },
  "suggests": {
    "another-mod": "*"
  },
  "breaks": {},
  "custom": {},
  "jars": [],
  "conflicts": {},
  "languageAdapters": {},
  "recommends": {},
  "contributors": []
}